<launch>

	       <node pkg="fanboat_ll" name="serial" type="fanboat_serial.py"/>
	       <node pkg="joy" name="joystick" output="screen" type="joy_node"/>
	       <node pkg="lab2" name="joy_constant" output="screen" type="joy_constant"/>
	       <node pkg="lab2" name="joyAngleIntegrater" output="screen" type="joyAngleIntegrater"/>
	       <node pkg="lab2" name="sensorFilter" type="sensorFilter"/>
	       <node pkg="lab2" name="reactiveControl" output="screen" type="reactiveControl"/>
	       <node pkg="lab2" name="arbitrator" output="screen" type="arbitrator"/>
	       <node pkg="lab2" name="angularPositionPID" type="angularPositionPID"/>
           <node pkg="lab2" name="triangle" type="triangle"/>
	       <param name="pValue" type="double" value="120"/>
	       <param name="dValue" type="double" value=".11"/>
	       <param name="iValue" type="double" value="0.0000"/> <!--0.0005 was working ok ish-->
           <param name="iMax" type="double" value="0.0"/>
	       <param name="rightScale" type="double" value="1"/>
	       <param name="leftScale" type="double" value="1.1"/>
</launch>
